{
  "manifest_version": 3,
  "name": "BookMark Manager",
  "version": "1.0.0",
  "description": "An AI-powered bookmark manager for Chrome",
  "permissions": [
    "bookmarks",
    "storage",
    "favicon"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}